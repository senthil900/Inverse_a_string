//input: senthilnathan
//output: nahtanlihtnes

import java.lang.*;
import java.io.*;
import java.util.*;

public class StringReverse {
	public static void main(String[] args)
	{
		String str = "senthilnathan";
		StringBuffer sbr = new StringBuffer(str);
		sbr.reverse();
		System.out.println(sbr);
	}
}
